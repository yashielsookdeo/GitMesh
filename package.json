{
  "name": "gitmesh",
  "displayName": "GitMesh",
  "description": "Bulk Git operations dashboard for managing multiple repositories",
  "version": "0.1.0",
  "publisher": "gitmesh",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "keywords": [
    "git",
    "bulk operations",
    "repository management",
    "multi-repo",
    "fetch",
    "checkout",
    "push",
    "reset"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yashielsookdeo/gitmesh.git"
  },
  "bugs": {
    "url": "https://github.com/yashielsookdeo/gitmesh/issues"
  },
  "homepage": "https://github.com/yashielsookdeo/gitmesh#readme",
  "license": "MIT",
  "main": "./out/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitmesh.openDashboard",
        "title": "GitMesh: Open Dashboard",
        "category": "GitMesh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && webpack --mode production",
    "watch": "tsc -watch -p ./ & webpack --watch --mode development",
    "package": "vsce package",
    "lint": "eslint src --ext ts,tsx"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@vscode/vsce": "^2.22.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
